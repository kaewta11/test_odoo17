<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="wizard_exchange_rate_sync_view" model="ir.ui.view">
        <field name="name">wizard.exchange.rate.sync.form</field>
        <field name="model">wizard.exchange.rate.sync</field>
        <field name="arch" type="xml">
            <form>
                <group>
                    <group>
                        <field name="period" readonly="1"/>
                        <field name="currency" readonly="1"/>
                        <field name="buying_sight" readonly="1"/>
                        <field name="buying_transfer" readonly="1"/>
                        <field name="selling" readonly="1"/>
                    </group>
                    <group>
                    </group>
                </group>
                <footer>
                    <button string="OK" special="cancel" class="btn-primary"/>
                </footer>
            </form>
        </field>
    </record>

</odoo>